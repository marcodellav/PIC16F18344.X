{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: clang build active file",
			"command": "/Applications/microchip/xc8/v2.31/bin/xc8-cc",
			"args": [
				"-mcpu=16f18344",
				"-O0",
				"-std=c99",
				"-gdwarf-3",
				"-mstack=compiled:auto:auto",
				"${file}",
				"-o",
				"${fileDirname}/${fileBasenameNoExtension}"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: /Applications/microchip/xc8/v2.31/bin/xc8-cc"
		}
	]
}